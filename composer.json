{
    "name": "jumpgate/users",
    "description": "Adds basic user functionality to a JumpGate site.",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Blasingame",
            "email": "tpblasingame@gmail.com"
        },
        {
            "name": "Brandon Hyde",
            "email": "hyde.brandon@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^5.0",
        "jumpgate/database": "*@dev",
        "jumpgate/core": "*@dev",
        "jumpgate/view-resolution": "^1.0",
        "laravelcollective/html": "^5.4",
        "kodeine/laravel-acl": "*@dev"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "~3.4",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "src/database/migrations",
            "src/database/seeds",
            "src/database/social_migrations"
        ],
        "psr-4": {
            "JumpGate\\Users\\": "src/JumpGate/Users"
        }
    },
    "autoload-dev": {
        "Tests\\": "tests"
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "path",
            "url": "../Core"
        },
        {
            "type": "path",
            "url": "../Database"
        },
        {
            "type": "path",
            "url": "../ACL"
        }
    ]
}
