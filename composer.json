{
    "name": "jumpgate/users",
    "description": "Adds basic user functionality to a JumpGate site.",
    "license": "MIT",
    "authors": [
        {
            "name": "Travis Blasingame",
            "email": "tpblasingame@gmail.com"
        },
        {
            "name": "Brandon Hyde",
            "email": "hyde.brandon@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": "^5.0",
        "jumpgate/core": "~1.0",
        "jumpgate/view-resolution": "^2.0",
        "laravelcollective/html": "^5.7",
        "kodeine/laravel-acl": "dev-master#6c9d73e"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "~3.7"
    },
    "autoload": {
        "classmap": [
            "src/database/migrations",
            "src/database/seeds",
            "src/database/social_migrations"
        ],
        "psr-4": {
            "JumpGate\\Users\\": "src/JumpGate/Users"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests",
            "App\\Models\\": "tests/resources"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JumpGate\\Users\\Providers\\UsersServiceProvider",
                "Kodeine\\Acl\\AclServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
