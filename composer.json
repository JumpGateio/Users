{
    "name": "jumpgate/users",
    "description": "Adds basic user functionality to a JumpGate site.",
    "license": "MIT",
    "authors": [
        {
            "name": "Stygian",
            "email": "stygian.warlock.v2@gmail.com"
        },
        {
            "name": "Riddles",
            "email": "riddles@dev-toolbox.com"
        }
    ],
    "require": {
        "laravel/framework": "^5.0",
        "jumpgate/database": "*@dev",
        "jumpgate/core": "*@dev",
        "jumpgate/view-resolution": "*@dev",
        "laravelcollective/html": "^5.4"
    },
    "autoload": {
        "classmap": [
            "src/database/migrations",
            "src/database/seeds",
            "src/database/social_migrations"
        ],
        "psr-4": {
            "JumpGate\\Users\\": "src/JumpGate/Users"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "path",
            "url": "../Users"
        },
        {
            "type": "path",
            "url": "../Core"
        },
        {
            "type": "path",
            "url": "../ViewResolution"
        },
        {
            "type": "path",
            "url": "../Database"
        }
    ]
}
